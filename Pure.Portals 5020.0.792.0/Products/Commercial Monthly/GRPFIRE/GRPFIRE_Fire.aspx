		var liMaxHeight = 0;
		var liMinHeight = 46;
		var liRowElement = 0;
		var recordArray = new Array();
		var arrayCount = 0;
		if ($("#id9aa33e4b81c242fa80b833a7f2d58850 div").attr("data-column-count") != "undefined")
		{
			columnCount = $("#id9aa33e4b81c242fa80b833a7f2d58850 div").attr("data-column-count");		
