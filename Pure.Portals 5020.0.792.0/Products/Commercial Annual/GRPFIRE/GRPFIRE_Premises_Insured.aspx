        			exp = new Expression("(GENERAL.CHILD_COUNTER > 0 && GENERAL.TRANSACTION_TYPE =='MTA' ) || GENERAL.TRANSACTION_TYPE =='NB' || GENERAL.TRANSACTION_TYPE == 'REN' || GENERAL.TRANSACTION_TYPE == 'MTC'");
