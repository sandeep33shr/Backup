        window.Colours.stripSingleQuotes = function(value){
        	if (value.slice(0,1) == "'" && value.slice(-1) == "'")
        		value = value.slice(1, -1);
        	return value;
        };
        
        
        String.prototype.killWhiteSpace = function() {
            return this.replace(/\s/g, '');
