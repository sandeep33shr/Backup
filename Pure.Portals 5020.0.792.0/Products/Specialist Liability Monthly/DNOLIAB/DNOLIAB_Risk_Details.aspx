	if (labelAlign != ""){
		switch (labelAlign.toLowerCase()){
			case "right": styleString += "#idccb31e50d478413aa1d808dd96ea4136 label{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#idccb31e50d478413aa1d808dd96ea4136 label{text-align:center;}"; break;
			case "left": 
			default: styleString += "#idccb31e50d478413aa1d808dd96ea4136 label{text-align:left;}"; break;
		}
	}
	if (textAlign != ""){
		switch (textAlign.toLowerCase()){
			case "right": styleString += "#idccb31e50d478413aa1d808dd96ea4136 input{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#idccb31e50d478413aa1d808dd96ea4136 input{text-align:center;}"; break;
			case "left": 
			default: styleString += "#idccb31e50d478413aa1d808dd96ea4136 input{text-align:left;}"; break;
		}
	}
	
	if (styleString != ""){
		goog.style.installStyles(styleString);
	}
