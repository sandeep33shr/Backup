	var labelWidth = "";	
	
	$(document).ready(function(){
		var liElementHeight = 0;	
		var liMaxHeight = 0;
		var liMinHeight = 46;
		var liRowElement = 0;
		var recordArray = new Array();
		var arrayCount = 0;
		if ($("#id09bd152cdc42462eaa5ddb9ba82a9ae3 div").attr("data-column-count") != "undefined")
		{
			columnCount = $("#id09bd152cdc42462eaa5ddb9ba82a9ae3 div").attr("data-column-count");		
		}
		
		if (columnCount > 1)
		{
			$("#id09bd152cdc42462eaa5ddb9ba82a9ae3 div ul li").each(function(){		  
			  liElementHeight = $(this).height();	
				
			  if (liElementHeight < liMinHeight)
			  {
				  liElementHeight = liMinHeight;			  
			  }
			  
			  if (liMaxHeight != 0 && liMaxHeight > liMinHeight)
			  {
				  if (liElementHeight > liMaxHeight)
				  {
					  liElementHeight = liMaxHeight;			  
				  }	
			  }			 

