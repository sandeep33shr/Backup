        		Colours.SetBackgroundColour(field, colour, condition, elseColour);
        	}
        })();
        /**
         * @fileoverview Listens and executes an expression when a control loses focus.
         * OnBlur
         */
        (function(){
        	
        	if (isOnLoad) {		
        		
        		// Setup an instance of the field
        		var field = Field.getInstance("CPLRISK", "LIM_IND");
        		
        		var valueExp = new Expression("CPLRISK.PREMIUM.setValue(CPLRISK.LIM_IND * CPLRISK.RATE * 0.01)");
