		var liMaxHeight = 0;
		var liMinHeight = 46;
		var liRowElement = 0;
		var recordArray = new Array();
		var arrayCount = 0;
		if ($("#id7909f1b6036447368e80a55531b7b1c6 div").attr("data-column-count") != "undefined")
