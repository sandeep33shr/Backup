<script type="text/javascript">
	var labelAlign = "";
	var textAlign = "";
	var labelWidth = "";	
	
	$(document).ready(function(){
		var liElementHeight = 0;	
		var liMaxHeight = 0;
		var liMinHeight = 46;
		var liRowElement = 0;
		var recordArray = new Array();
		var arrayCount = 0;
		if ($("#id79b71b6c998d48cebb80d4c9e983ce5c div").attr("data-column-count") != "undefined")
		{
			columnCount = $("#id79b71b6c998d48cebb80d4c9e983ce5c div").attr("data-column-count");		
		}
		
		if (columnCount > 1)
		{
			$("#id79b71b6c998d48cebb80d4c9e983ce5c div ul li").each(function(){		  
			  liElementHeight = $(this).height();	
				
			  if (liElementHeight < liMinHeight)
			  {
				  liElementHeight = liMinHeight;			  
			  }
			  
			  if (liMaxHeight != 0 && liMaxHeight > liMinHeight)
			  {
				  if (liElementHeight > liMaxHeight)
				  {
					  liElementHeight = liMaxHeight;			  
				  }	
			  }			 

			  if (liRowElement == (columnCount -1))
			  {
				  liRowElement = 0;			 
				  recordArray[arrayCount] = liElementHeight;		  
				  arrayCount++;
				  liElementHeight = 0;
				  
			  }
			  else{
				  liRowElement++;
			  }		
			  
			});
			
			liRowElement =0;
			arrayCount= 0;
			$("#id79b71b6c998d48cebb80d4c9e983ce5c div ul li").each(function(){		  
			  $(this).height(recordArray[arrayCount]);
			  if (liRowElement == (columnCount -1))
			   {
				liRowElement = 0;
				arrayCount++;
			   }
			  else{
				liRowElement++;
			  } 		  
			});
			}
	});	
	
	var styleString = "";
	if (labelWidth != ""){
		if ((new Expression("IsNumeric('" + labelWidth + "')")).valueOf()){
			labelWidth = labelWidth + "px";
		}
		styleString += "#id79b71b6c998d48cebb80d4c9e983ce5c label{width: " + labelWidth + ";}";
	}
	if (labelAlign != ""){
		switch (labelAlign.toLowerCase()){
			case "right": styleString += "#id79b71b6c998d48cebb80d4c9e983ce5c label{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#id79b71b6c998d48cebb80d4c9e983ce5c label{text-align:center;}"; break;
			case "left": 
			default: styleString += "#id79b71b6c998d48cebb80d4c9e983ce5c label{text-align:left;}"; break;
		}
	}
	if (textAlign != ""){
		switch (textAlign.toLowerCase()){
			case "right": styleString += "#id79b71b6c998d48cebb80d4c9e983ce5c input{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#id79b71b6c998d48cebb80d4c9e983ce5c input{text-align:center;}"; break;
			case "left": 
			default: styleString += "#id79b71b6c998d48cebb80d4c9e983ce5c input{text-align:left;}"; break;
		}
	}
	
	if (styleString != ""){
		goog.style.installStyles(styleString);
	}
</script>
