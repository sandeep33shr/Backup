			default: styleString += "#id5aa889c9fa8c4b99a620438d0198e7ec label{text-align:left;}"; break;
		}
	}
	if (textAlign != ""){
		switch (textAlign.toLowerCase()){
			case "right": styleString += "#id5aa889c9fa8c4b99a620438d0198e7ec input{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#id5aa889c9fa8c4b99a620438d0198e7ec input{text-align:center;}"; break;
			case "left": 
			default: styleString += "#id5aa889c9fa8c4b99a620438d0198e7ec input{text-align:left;}"; break;
		}
	}
	
	if (styleString != ""){
		goog.style.installStyles(styleString);
	}
</script>
