			case "centre":
			case "center":
			case "middle": styleString += "#idf31b20bffded4386b1c8b5058fe1e8d4 label{text-align:center;}"; break;
			case "left": 
			default: styleString += "#idf31b20bffded4386b1c8b5058fe1e8d4 label{text-align:left;}"; break;
		}
	}
	if (textAlign != ""){
		switch (textAlign.toLowerCase()){
			case "right": styleString += "#idf31b20bffded4386b1c8b5058fe1e8d4 input{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#idf31b20bffded4386b1c8b5058fe1e8d4 input{text-align:center;}"; break;
			case "left": 
			default: styleString += "#idf31b20bffded4386b1c8b5058fe1e8d4 input{text-align:left;}"; break;
		}
	}
	
	if (styleString != ""){
		goog.style.installStyles(styleString);
	}
