		var liMaxHeight = 0;
		var liMinHeight = 46;
		var liRowElement = 0;
		var recordArray = new Array();
		var arrayCount = 0;
		if ($("#id0fdde16a030643a6b7187d299ed21377 div").attr("data-column-count") != "undefined")
		{
