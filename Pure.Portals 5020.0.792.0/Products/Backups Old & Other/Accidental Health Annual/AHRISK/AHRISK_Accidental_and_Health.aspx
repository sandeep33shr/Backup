		}
	}
	if (textAlign != ""){
		switch (textAlign.toLowerCase()){
			case "right": styleString += "#id3ecf672c09bf42c299bd3d5dd9288cb8 input{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#id3ecf672c09bf42c299bd3d5dd9288cb8 input{text-align:center;}"; break;
			case "left": 
