		var liMaxHeight = 0;
		var liMinHeight = 46;
		var liRowElement = 0;
		var recordArray = new Array();
		var arrayCount = 0;
		if ($("#ida7cf3a5e2d434efd915f96e64896f5c5 div").attr("data-column-count") != "undefined")
