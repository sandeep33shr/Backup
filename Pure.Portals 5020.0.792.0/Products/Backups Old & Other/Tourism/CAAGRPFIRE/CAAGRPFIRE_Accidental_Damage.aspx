	var textAlign = "";
	var labelWidth = "";	
	
	$(document).ready(function(){
		var liElementHeight = 0;	
		var liMaxHeight = 0;
		var liMinHeight = 46;
		var liRowElement = 0;
		var recordArray = new Array();
		var arrayCount = 0;
		if ($("#idedefd0d2cd3d43c38ca44981aa19c12b div").attr("data-column-count") != "undefined")
		{
			columnCount = $("#idedefd0d2cd3d43c38ca44981aa19c12b div").attr("data-column-count");		
		}
		
		if (columnCount > 1)
		{
			$("#idedefd0d2cd3d43c38ca44981aa19c12b div ul li").each(function(){		  
			  liElementHeight = $(this).height();	
				
			  if (liElementHeight < liMinHeight)
			  {
				  liElementHeight = liMinHeight;			  
			  }
			  
			  if (liMaxHeight != 0 && liMaxHeight > liMinHeight)
			  {
				  if (liElementHeight > liMaxHeight)
				  {
					  liElementHeight = liMaxHeight;			  
				  }	
			  }			 

			  if (liRowElement == (columnCount -1))
			  {
				  liRowElement = 0;			 
				  recordArray[arrayCount] = liElementHeight;		  
				  arrayCount++;
				  liElementHeight = 0;
				  
			  }
			  else{
				  liRowElement++;
			  }		
			  
			});
			
			liRowElement =0;
			arrayCount= 0;
			$("#idedefd0d2cd3d43c38ca44981aa19c12b div ul li").each(function(){		  
			  $(this).height(recordArray[arrayCount]);
			  if (liRowElement == (columnCount -1))
			   {
				liRowElement = 0;
				arrayCount++;
			   }
			  else{
				liRowElement++;
			  } 		  
			});
			}
	});	
	
	var styleString = "";
	if (labelWidth != ""){
		if ((new Expression("IsNumeric('" + labelWidth + "')")).valueOf()){
			labelWidth = labelWidth + "px";
		}
		styleString += "#idedefd0d2cd3d43c38ca44981aa19c12b label{width: " + labelWidth + ";}";
	}
	if (labelAlign != ""){
		switch (labelAlign.toLowerCase()){
			case "right": styleString += "#idedefd0d2cd3d43c38ca44981aa19c12b label{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#idedefd0d2cd3d43c38ca44981aa19c12b label{text-align:center;}"; break;
			case "left": 
			default: styleString += "#idedefd0d2cd3d43c38ca44981aa19c12b label{text-align:left;}"; break;
		}
	}
	if (textAlign != ""){
		switch (textAlign.toLowerCase()){
			case "right": styleString += "#idedefd0d2cd3d43c38ca44981aa19c12b input{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#idedefd0d2cd3d43c38ca44981aa19c12b input{text-align:center;}"; break;
			case "left": 
			default: styleString += "#idedefd0d2cd3d43c38ca44981aa19c12b input{text-align:left;}"; break;
		}
	}
	
	if (styleString != ""){
		goog.style.installStyles(styleString);
	}
</script>
