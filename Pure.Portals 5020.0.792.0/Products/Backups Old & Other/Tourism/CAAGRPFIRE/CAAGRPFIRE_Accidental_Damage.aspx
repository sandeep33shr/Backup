			case "centre":
			case "center":
			case "middle": styleString += "#ide4db844011894cc8928c8ba7313c577c input{text-align:center;}"; break;
			case "left": 
			default: styleString += "#ide4db844011894cc8928c8ba7313c577c input{text-align:left;}"; break;
		}
	}
	
	if (styleString != ""){
