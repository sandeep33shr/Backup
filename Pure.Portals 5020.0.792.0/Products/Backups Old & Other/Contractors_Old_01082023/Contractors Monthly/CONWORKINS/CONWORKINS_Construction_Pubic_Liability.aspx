<script type="text/javascript">
	var labelAlign = "";
	var textAlign = "";
	var labelWidth = "";	
	
	$(document).ready(function(){
		var liElementHeight = 0;	
		var liMaxHeight = 0;
		var liMinHeight = 46;
		var liRowElement = 0;
		var recordArray = new Array();
		var arrayCount = 0;

		if ($("#idc78e0fc96f41470e9e40953ee901db7a div").attr("data-column-count") != "undefined")
		{
			columnCount = $("#idc78e0fc96f41470e9e40953ee901db7a div").attr("data-column-count");		
		}
		
		if (columnCount > 1)
		{
			$("#idc78e0fc96f41470e9e40953ee901db7a div ul li").each(function(){		  
			  liElementHeight = $(this).height();	
				
			  if (liElementHeight < liMinHeight)
			  {
				  liElementHeight = liMinHeight;			  
			  }
			  
			  if (liMaxHeight != 0 && liMaxHeight > liMinHeight)
			  {
				  if (liElementHeight > liMaxHeight)
				  {
					  liElementHeight = liMaxHeight;			  
				  }	
			  }			 

			  if (liRowElement == (columnCount -1))
			  {
				  liRowElement = 0;			 
				  recordArray[arrayCount] = liElementHeight;		  
				  arrayCount++;
				  liElementHeight = 0;
				  
			  }
			  else{
				  liRowElement++;
			  }		
			  
			});
			
			liRowElement =0;
			arrayCount= 0;
			$("#idc78e0fc96f41470e9e40953ee901db7a div ul li").each(function(){		  
			  $(this).height(recordArray[arrayCount]);
			  if (liRowElement == (columnCount -1))
			   {
				liRowElement = 0;
				arrayCount++;
			   }
			  else{
				liRowElement++;
			  } 		  
			});
			}
	});	
	
	var styleString = "";
	if (labelWidth != ""){
		if ((new Expression("IsNumeric('" + labelWidth + "')")).valueOf()){
			labelWidth = labelWidth + "px";
		}
		styleString += "#idc78e0fc96f41470e9e40953ee901db7a label{width: " + labelWidth + ";}";
	}
	if (labelAlign != ""){
		switch (labelAlign.toLowerCase()){
			case "right": styleString += "#idc78e0fc96f41470e9e40953ee901db7a label{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#idc78e0fc96f41470e9e40953ee901db7a label{text-align:center;}"; break;
			case "left": 
			default: styleString += "#idc78e0fc96f41470e9e40953ee901db7a label{text-align:left;}"; break;
		}
	}
	if (textAlign != ""){
		switch (textAlign.toLowerCase()){
			case "right": styleString += "#idc78e0fc96f41470e9e40953ee901db7a input{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#idc78e0fc96f41470e9e40953ee901db7a input{text-align:center;}"; break;
			case "left": 
			default: styleString += "#idc78e0fc96f41470e9e40953ee901db7a input{text-align:left;}"; break;
		}
	}
	
	if (styleString != ""){
		goog.style.installStyles(styleString);
	}
</script>
