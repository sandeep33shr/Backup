        			} else {
        				exp = new Expression(screenObjectStr + "[" + strObject + "] #" + type + " (" + condition + ") ? (" + strObject + "." + strProperty + ")");
        			}
        			
        			var field = Field.getInstance("CHLDSCR", "ADD_PREMIUM");
        			field.setValue(exp.getValue());
        		}
        	};
