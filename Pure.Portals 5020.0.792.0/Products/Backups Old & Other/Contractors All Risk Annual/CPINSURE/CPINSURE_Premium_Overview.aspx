		}
		styleString += "#id6c23b252bc67401bb4812b005bd6ce7f label{width: " + labelWidth + ";}";
	}
	if (labelAlign != ""){
		switch (labelAlign.toLowerCase()){
			case "right": styleString += "#id6c23b252bc67401bb4812b005bd6ce7f label{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#id6c23b252bc67401bb4812b005bd6ce7f label{text-align:center;}"; break;
			case "left": 
			default: styleString += "#id6c23b252bc67401bb4812b005bd6ce7f label{text-align:left;}"; break;
		}
	}
	if (textAlign != ""){
