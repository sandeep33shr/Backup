			case "middle": styleString += "#id78d8bada434d4dafb3de9f03f564bed9 label{text-align:center;}"; break;
			case "left": 
			default: styleString += "#id78d8bada434d4dafb3de9f03f564bed9 label{text-align:left;}"; break;
		}
	}
	if (textAlign != ""){
