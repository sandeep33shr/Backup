			case "center":
			case "middle": styleString += "#id6c23b252bc67401bb4812b005bd6ce7f input{text-align:center;}"; break;
			case "left": 
			default: styleString += "#id6c23b252bc67401bb4812b005bd6ce7f input{text-align:left;}"; break;
		}
	}
	
	if (styleString != ""){
		goog.style.installStyles(styleString);
	}
