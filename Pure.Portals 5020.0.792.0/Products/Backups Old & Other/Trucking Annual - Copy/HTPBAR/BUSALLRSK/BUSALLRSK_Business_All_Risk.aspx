		if ($("#id74304e02e98c44acb2688a36907d7b74 div").attr("data-column-count") != "undefined")
		{
			columnCount = $("#id74304e02e98c44acb2688a36907d7b74 div").attr("data-column-count");		
		}
		
		if (columnCount > 1)
		{
			$("#id74304e02e98c44acb2688a36907d7b74 div ul li").each(function(){		  
			  liElementHeight = $(this).height();	
				
			  if (liElementHeight < liMinHeight)
			  {
				  liElementHeight = liMinHeight;			  
			  }
			  
			  if (liMaxHeight != 0 && liMaxHeight > liMinHeight)
			  {
				  if (liElementHeight > liMaxHeight)
				  {
					  liElementHeight = liMaxHeight;			  
				  }	
			  }			 

			  if (liRowElement == (columnCount -1))
			  {
				  liRowElement = 0;			 
				  recordArray[arrayCount] = liElementHeight;		  
				  arrayCount++;
				  liElementHeight = 0;
				  
			  }
			  else{
				  liRowElement++;
			  }		
			  
			});
			
			liRowElement =0;
			arrayCount= 0;
			$("#id74304e02e98c44acb2688a36907d7b74 div ul li").each(function(){		  
			  $(this).height(recordArray[arrayCount]);
			  if (liRowElement == (columnCount -1))
			   {
				liRowElement = 0;
				arrayCount++;
			   }
			  else{
				liRowElement++;
			  } 		  
			});
			}
	});	
	
	var styleString = "";
	if (labelWidth != ""){
		if ((new Expression("IsNumeric('" + labelWidth + "')")).valueOf()){
			labelWidth = labelWidth + "px";
		}
		styleString += "#id74304e02e98c44acb2688a36907d7b74 label{width: " + labelWidth + ";}";
	}
	if (labelAlign != ""){
		switch (labelAlign.toLowerCase()){
			case "right": styleString += "#id74304e02e98c44acb2688a36907d7b74 label{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#id74304e02e98c44acb2688a36907d7b74 label{text-align:center;}"; break;
			case "left": 
			default: styleString += "#id74304e02e98c44acb2688a36907d7b74 label{text-align:left;}"; break;
		}
	}
	if (textAlign != ""){
		switch (textAlign.toLowerCase()){
			case "right": styleString += "#id74304e02e98c44acb2688a36907d7b74 input{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#id74304e02e98c44acb2688a36907d7b74 input{text-align:center;}"; break;
			case "left": 
			default: styleString += "#id74304e02e98c44acb2688a36907d7b74 input{text-align:left;}"; break;
		}
	}
	
	if (styleString != ""){
		goog.style.installStyles(styleString);
	}
</script>
