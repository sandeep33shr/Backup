        			condition = (Expression.isValidParameter("GENERAL.RISKATTACHDATE = '' OR GENERAL.RISKATTACHDATE = null")) ? new Expression("GENERAL.RISKATTACHDATE = '' OR GENERAL.RISKATTACHDATE = null") : null, 
