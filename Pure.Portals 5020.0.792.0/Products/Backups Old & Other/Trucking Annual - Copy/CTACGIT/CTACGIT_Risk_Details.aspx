				  recordArray[arrayCount] = liElementHeight;		  
				  arrayCount++;
				  liElementHeight = 0;
				  
			  }
			  else{
				  liRowElement++;
			  }		
			  
			});
			
			liRowElement =0;
			arrayCount= 0;
			$("#id1699c5953c41495cb7f553bbde7946ea div ul li").each(function(){		  
			  $(this).height(recordArray[arrayCount]);
