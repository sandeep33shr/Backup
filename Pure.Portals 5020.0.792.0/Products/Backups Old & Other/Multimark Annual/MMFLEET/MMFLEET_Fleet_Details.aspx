			case "centre":
			case "center":
			case "middle": styleString += "#id68d3493c28564e51861a87f19f135bef input{text-align:center;}"; break;
			case "left": 
			default: styleString += "#id68d3493c28564e51861a87f19f135bef input{text-align:left;}"; break;
		}
	}
	
	if (styleString != ""){
		goog.style.installStyles(styleString);
	}
