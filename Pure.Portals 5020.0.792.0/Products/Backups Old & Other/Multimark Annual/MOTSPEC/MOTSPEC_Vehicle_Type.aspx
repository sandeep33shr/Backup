		var liMaxHeight = 0;
		var liMinHeight = 46;
		var liRowElement = 0;
		var recordArray = new Array();
		var arrayCount = 0;
		if ($("#idae85831e37214de8b14a8668a8e211fc div").attr("data-column-count") != "undefined")
		{
