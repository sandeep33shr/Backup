		var liElementHeight = 0;	
		var liMaxHeight = 0;
		var liMinHeight = 46;
		var liRowElement = 0;
		var recordArray = new Array();
		var arrayCount = 0;
		if ($("#iddb41a68b1ab346c78ebd68a7f655affc div").attr("data-column-count") != "undefined")
		{
			columnCount = $("#iddb41a68b1ab346c78ebd68a7f655affc div").attr("data-column-count");		
		}
		
		if (columnCount > 1)
		{
			$("#iddb41a68b1ab346c78ebd68a7f655affc div ul li").each(function(){		  
			  liElementHeight = $(this).height();	
				
			  if (liElementHeight < liMinHeight)
			  {
				  liElementHeight = liMinHeight;			  
			  }
			  
			  if (liMaxHeight != 0 && liMaxHeight > liMinHeight)
			  {
				  if (liElementHeight > liMaxHeight)
				  {
					  liElementHeight = liMaxHeight;			  
				  }	
			  }			 

			  if (liRowElement == (columnCount -1))
			  {
				  liRowElement = 0;			 
				  recordArray[arrayCount] = liElementHeight;		  
				  arrayCount++;
				  liElementHeight = 0;
				  
			  }
			  else{
				  liRowElement++;
			  }		
			  
			});
			
			liRowElement =0;
			arrayCount= 0;
			$("#iddb41a68b1ab346c78ebd68a7f655affc div ul li").each(function(){		  
			  $(this).height(recordArray[arrayCount]);
			  if (liRowElement == (columnCount -1))
			   {
				liRowElement = 0;
				arrayCount++;
			   }
			  else{
				liRowElement++;
			  } 		  
			});
			}
	});	
	
	var styleString = "";
	if (labelWidth != ""){
		if ((new Expression("IsNumeric('" + labelWidth + "')")).valueOf()){
			labelWidth = labelWidth + "px";
		}
		styleString += "#iddb41a68b1ab346c78ebd68a7f655affc label{width: " + labelWidth + ";}";
	}
	if (labelAlign != ""){
		switch (labelAlign.toLowerCase()){
			case "right": styleString += "#iddb41a68b1ab346c78ebd68a7f655affc label{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#iddb41a68b1ab346c78ebd68a7f655affc label{text-align:center;}"; break;
			case "left": 
			default: styleString += "#iddb41a68b1ab346c78ebd68a7f655affc label{text-align:left;}"; break;
		}
	}
	if (textAlign != ""){
		switch (textAlign.toLowerCase()){
			case "right": styleString += "#iddb41a68b1ab346c78ebd68a7f655affc input{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#iddb41a68b1ab346c78ebd68a7f655affc input{text-align:center;}"; break;
			case "left": 
			default: styleString += "#iddb41a68b1ab346c78ebd68a7f655affc input{text-align:left;}"; break;
		}
	}
	
	if (styleString != ""){
		goog.style.installStyles(styleString);
	}
</script>
