			case "middle": styleString += "#id902a8f6530874807bee7320e61dc612d label{text-align:center;}"; break;
			case "left": 
			default: styleString += "#id902a8f6530874807bee7320e61dc612d label{text-align:left;}"; break;
		}
	}
	if (textAlign != ""){
