		var liMaxHeight = 0;
		var liMinHeight = 46;
		var liRowElement = 0;
		var recordArray = new Array();
		var arrayCount = 0;
		if ($("#id523799b6df77441aba618eb10db3ea45 div").attr("data-column-count") != "undefined")
		{
			columnCount = $("#id523799b6df77441aba618eb10db3ea45 div").attr("data-column-count");		
