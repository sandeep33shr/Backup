			  else{
				liRowElement++;
			  } 		  
			});
			}
	});	
	
	var styleString = "";
	if (labelWidth != ""){
		if ((new Expression("IsNumeric('" + labelWidth + "')")).valueOf()){
			labelWidth = labelWidth + "px";
		}
		styleString += "#id996266887b6b40cea576db59fbd3cad2 label{width: " + labelWidth + ";}";
	}
	if (labelAlign != ""){
		switch (labelAlign.toLowerCase()){
			case "right": styleString += "#id996266887b6b40cea576db59fbd3cad2 label{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#id996266887b6b40cea576db59fbd3cad2 label{text-align:center;}"; break;
			case "left": 
			default: styleString += "#id996266887b6b40cea576db59fbd3cad2 label{text-align:left;}"; break;
		}
	}
	if (textAlign != ""){
		switch (textAlign.toLowerCase()){
			case "right": styleString += "#id996266887b6b40cea576db59fbd3cad2 input{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#id996266887b6b40cea576db59fbd3cad2 input{text-align:center;}"; break;
			case "left": 
			default: styleString += "#id996266887b6b40cea576db59fbd3cad2 input{text-align:left;}"; break;
		}
	}
	
	if (styleString != ""){
		goog.style.installStyles(styleString);
	}
