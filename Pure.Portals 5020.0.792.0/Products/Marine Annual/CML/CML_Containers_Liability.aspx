		styleString += "#id78ed743f8a2d4921b07785ad6d45f0ec label{width: " + labelWidth + ";}";
	}
	if (labelAlign != ""){
		switch (labelAlign.toLowerCase()){
			case "right": styleString += "#id78ed743f8a2d4921b07785ad6d45f0ec label{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#id78ed743f8a2d4921b07785ad6d45f0ec label{text-align:center;}"; break;
			case "left": 
			default: styleString += "#id78ed743f8a2d4921b07785ad6d45f0ec label{text-align:left;}"; break;
		}
	}
	if (textAlign != ""){
		switch (textAlign.toLowerCase()){
			case "right": styleString += "#id78ed743f8a2d4921b07785ad6d45f0ec input{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#id78ed743f8a2d4921b07785ad6d45f0ec input{text-align:center;}"; break;
			case "left": 
			default: styleString += "#id78ed743f8a2d4921b07785ad6d45f0ec input{text-align:left;}"; break;
		}
	}
	
	if (styleString != ""){
		goog.style.installStyles(styleString);
	}
