			case "centre":
			case "center":
			case "middle": styleString += "#id90bba9e4dcb740b19d4c3a74b19a1183 label{text-align:center;}"; break;
			case "left": 
			default: styleString += "#id90bba9e4dcb740b19d4c3a74b19a1183 label{text-align:left;}"; break;
		}
	}
	if (textAlign != ""){
		switch (textAlign.toLowerCase()){
			case "right": styleString += "#id90bba9e4dcb740b19d4c3a74b19a1183 input{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#id90bba9e4dcb740b19d4c3a74b19a1183 input{text-align:center;}"; break;
			case "left": 
			default: styleString += "#id90bba9e4dcb740b19d4c3a74b19a1183 input{text-align:left;}"; break;
		}
	}
	
	if (styleString != ""){
		goog.style.installStyles(styleString);
	}
