	if (labelWidth != ""){
		if ((new Expression("IsNumeric('" + labelWidth + "')")).valueOf()){
			labelWidth = labelWidth + "px";
		}
		styleString += "#id90bba9e4dcb740b19d4c3a74b19a1183 label{width: " + labelWidth + ";}";
