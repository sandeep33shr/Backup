        /**
         * @fileoverview
         * Read Only, make the field Read Only
         */
        (function(){
        	
        	var field = Field.getInstance("EXTCLM", "OTHERADD");
        	field.setReadOnly(true);
        })();
        /** 
         * ToggleContainer
         * @param frmOther The element to toggle
         * @param {1} True if the element should be toggle'd when the control is unticked instead of ticked.
         * Defaults to false.
         */
        (function(){
        	
        	
        	if (isOnLoad) {
        		var field = Field.getInstance("EXTCLM","OTHERADD");
        	
        		var inverse = (Expression.isValidParameter("{1}") && ("{1}".toLowerCase() == "true")) ? true : false;
        		var update = function(){
        			var value = (field.getValue() != true) ? "false" : "true";
        			
        			if (!inverse){
        				(new Expression("SetElementDisplay('frmOther', !!" + value + ")")).valueOf();
        			} else {
        				(new Expression("SetElementDisplay('frmOther', !" + value + ")")).valueOf();
        			}
        		};
        		events.listen(Field.getInstance("EXTCLM", "OTHERADD"), "change", update);
        		update();
        	}
        
        })();
