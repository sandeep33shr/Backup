	$(document).ready(function(){
		var liElementHeight = 0;	
		var liMaxHeight = 0;
		var liRowElement = 0;
		var recordArray = new Array();
		var arrayCount = 0;
		if ($("#frmExtDetails div").attr("data-column-count") != "undefined")
		{
			columnCount = $("#frmExtDetails div").attr("data-column-count");		
		}
		
		if (columnCount > 1)
		{
			$("#frmExtDetails div ul li").each(function(){		  
			  liElementHeight = $(this).height();	  
			  if (liElementHeight > liMaxHeight)
			  {
				  liMaxHeight = liElementHeight;			  
			  }	

			  if (liRowElement == (columnCount -1))
			  {
				  liRowElement = 0;			 
				  recordArray[arrayCount] = liMaxHeight;		  
				  arrayCount++;
				  liMaxHeight = 0;
				  
			  }
			  else{
				  liRowElement++;
			  }		
			  
			});
			
			liRowElement =0;
			arrayCount= 0;
			$("#frmExtDetails div ul li").each(function(){		  
			  $(this).height(recordArray[arrayCount]);
			  if (liRowElement == (columnCount -1))
			   {
				liRowElement = 0;
				arrayCount++;
			   }
			  else{
				liRowElement++;
			  } 		  
			});
			}
	});	
	
	var styleString = "";
	if (labelWidth != ""){
		if ((new Expression("IsNumeric('" + labelWidth + "')")).valueOf()){
			labelWidth = labelWidth + "px";
		}
		styleString += "#frmExtDetails label{width: " + labelWidth + ";}";
	}
	if (labelAlign != ""){
		switch (labelAlign.toLowerCase()){
			case "right": styleString += "#frmExtDetails label{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#frmExtDetails label{text-align:center;}"; break;
			case "left": 
			default: styleString += "#frmExtDetails label{text-align:left;}"; break;
		}
	}
	if (textAlign != ""){
		switch (textAlign.toLowerCase()){
			case "right": styleString += "#frmExtDetails input{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#frmExtDetails input{text-align:center;}"; break;
			case "left": 
			default: styleString += "#frmExtDetails input{text-align:left;}"; break;
		}
	}
	
	if (styleString != ""){
		goog.style.installStyles(styleString);
	}
