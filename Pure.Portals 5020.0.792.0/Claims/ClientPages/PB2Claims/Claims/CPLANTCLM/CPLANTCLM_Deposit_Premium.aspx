		var liElementHeight = 0;	
		var liMaxHeight = 0;
		var liRowElement = 0;
		var recordArray = new Array();
		var arrayCount = 0;
		if ($("#id04752c914a744e5e8c8b540884097412 div").attr("data-column-count") != "undefined")
