		var liMaxHeight = 0;
		var liMinHeight = 46;
		var liRowElement = 0;
		var recordArray = new Array();
		var arrayCount = 0;
		if ($("#idc68af04a541045b9a62f98e3e0e783d5 div").attr("data-column-count") != "undefined")
