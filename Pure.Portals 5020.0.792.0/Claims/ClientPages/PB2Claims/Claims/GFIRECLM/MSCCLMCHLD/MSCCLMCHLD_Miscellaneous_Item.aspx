		var arrayCount = 0;
		if ($("#idc4e15ddba09f44cfb9f4e40c1292cc85 div").attr("data-column-count") != "undefined")
		{
			columnCount = $("#idc4e15ddba09f44cfb9f4e40c1292cc85 div").attr("data-column-count");		
		}
		
		if (columnCount > 1)
		{
			$("#idc4e15ddba09f44cfb9f4e40c1292cc85 div ul li").each(function(){		  
			  liElementHeight = $(this).height();	
				
			  if (liElementHeight < liMinHeight)
			  {
				  liElementHeight = liMinHeight;			  
			  }
			  
			  if (liMaxHeight != 0 && liMaxHeight > liMinHeight)
			  {
				  if (liElementHeight > liMaxHeight)
				  {
					  liElementHeight = liMaxHeight;			  
				  }	
			  }			 

			  if (liRowElement == (columnCount -1))
			  {
				  liRowElement = 0;			 
				  recordArray[arrayCount] = liElementHeight;		  
				  arrayCount++;
				  liElementHeight = 0;
				  
			  }
			  else{
				  liRowElement++;
			  }		
			  
			});
			
			liRowElement =0;
			arrayCount= 0;
			$("#idc4e15ddba09f44cfb9f4e40c1292cc85 div ul li").each(function(){		  
			  $(this).height(recordArray[arrayCount]);
			  if (liRowElement == (columnCount -1))
			   {
				liRowElement = 0;
				arrayCount++;
			   }
			  else{
				liRowElement++;
			  } 		  
			});
			}
	});	
	
	var styleString = "";
	if (labelWidth != ""){
		if ((new Expression("IsNumeric('" + labelWidth + "')")).valueOf()){
			labelWidth = labelWidth + "px";
		}
		styleString += "#idc4e15ddba09f44cfb9f4e40c1292cc85 label{width: " + labelWidth + ";}";
	}
	if (labelAlign != ""){
		switch (labelAlign.toLowerCase()){
			case "right": styleString += "#idc4e15ddba09f44cfb9f4e40c1292cc85 label{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#idc4e15ddba09f44cfb9f4e40c1292cc85 label{text-align:center;}"; break;
			case "left": 
			default: styleString += "#idc4e15ddba09f44cfb9f4e40c1292cc85 label{text-align:left;}"; break;
		}
	}
	if (textAlign != ""){
		switch (textAlign.toLowerCase()){
			case "right": styleString += "#idc4e15ddba09f44cfb9f4e40c1292cc85 input{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#idc4e15ddba09f44cfb9f4e40c1292cc85 input{text-align:center;}"; break;
			case "left": 
			default: styleString += "#idc4e15ddba09f44cfb9f4e40c1292cc85 input{text-align:left;}"; break;
		}
	}
	
	if (styleString != ""){
		goog.style.installStyles(styleString);
	}
</script>
