		var liMaxHeight = 0;
		var liMinHeight = 46;
		var liRowElement = 0;
		var recordArray = new Array();
		var arrayCount = 0;
		if ($("#frmCvrDetails div").attr("data-column-count") != "undefined")
		{
			columnCount = $("#frmCvrDetails div").attr("data-column-count");		
		}
		
		if (columnCount > 1)
