        	if (isOnLoad) {
        		var field = Field.getInstance("TRADERSCLAIM","SECTIONB");
        	
        		var inverse = (Expression.isValidParameter("{1}") && ("{1}".toLowerCase() == "true")) ? true : false;
        		var update = function(){
        			var value = (field.getValue() != true) ? "false" : "true";
        			
        			if (!inverse){
        				(new Expression("SetElementDisplay('SubsectionB', !!" + value + ")")).valueOf();
        			} else {
        				(new Expression("SetElementDisplay('SubsectionB', !" + value + ")")).valueOf();
        			}
        		};
        		events.listen(Field.getInstance("TRADERSCLAIM", "SECTIONB"), "change", update);
        		update();
        	}
        
        })();
