<script type="text/javascript">
	var labelAlign = "";
	var textAlign = "";
	var labelWidth = "";	
	
	$(document).ready(function(){
		var liElementHeight = 0;	
		var liMaxHeight = 0;
		var liRowElement = 0;
		var recordArray = new Array();
		var arrayCount = 0;
		if ($("#id25652835e2af44f4aab2dfaf7b74171b div").attr("data-column-count") != "undefined")
		{
			columnCount = $("#id25652835e2af44f4aab2dfaf7b74171b div").attr("data-column-count");		
		}
		
		if (columnCount > 1)
		{
			$("#id25652835e2af44f4aab2dfaf7b74171b div ul li").each(function(){		  
			  liElementHeight = $(this).height();	  
			  if (liElementHeight > liMaxHeight)
			  {
				  liMaxHeight = liElementHeight;			  
			  }	

			  if (liRowElement == (columnCount -1))
			  {
				  liRowElement = 0;			 
				  recordArray[arrayCount] = liMaxHeight;		  
				  arrayCount++;
				  liMaxHeight = 0;
				  
			  }
			  else{
				  liRowElement++;
			  }		
			  
			});
			
			liRowElement =0;
			arrayCount= 0;
			$("#id25652835e2af44f4aab2dfaf7b74171b div ul li").each(function(){		  
			  $(this).height(recordArray[arrayCount]);
			  if (liRowElement == (columnCount -1))
			   {
				liRowElement = 0;
				arrayCount++;
			   }
			  else{
				liRowElement++;
			  } 		  
			});
			}
	});	
	
	var styleString = "";
	if (labelWidth != ""){
		if ((new Expression("IsNumeric('" + labelWidth + "')")).valueOf()){
			labelWidth = labelWidth + "px";
		}
		styleString += "#id25652835e2af44f4aab2dfaf7b74171b label{width: " + labelWidth + ";}";
	}
	if (labelAlign != ""){
		switch (labelAlign.toLowerCase()){
			case "right": styleString += "#id25652835e2af44f4aab2dfaf7b74171b label{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#id25652835e2af44f4aab2dfaf7b74171b label{text-align:center;}"; break;
			case "left": 
			default: styleString += "#id25652835e2af44f4aab2dfaf7b74171b label{text-align:left;}"; break;
		}
	}
	if (textAlign != ""){
		switch (textAlign.toLowerCase()){
			case "right": styleString += "#id25652835e2af44f4aab2dfaf7b74171b input{text-align:right;}"; break;
			case "centre":
			case "center":
			case "middle": styleString += "#id25652835e2af44f4aab2dfaf7b74171b input{text-align:center;}"; break;
			case "left": 
			default: styleString += "#id25652835e2af44f4aab2dfaf7b74171b input{text-align:left;}"; break;
		}
	}
	
	if (styleString != ""){
		goog.style.installStyles(styleString);
	}
</script>
