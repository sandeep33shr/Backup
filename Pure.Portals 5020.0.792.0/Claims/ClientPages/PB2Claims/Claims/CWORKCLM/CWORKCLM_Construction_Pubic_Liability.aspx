			labelWidth = labelWidth + "px";
		}
		styleString += "#id9bf062396dfd40fa9b48925b4216cb8a label{width: " + labelWidth + ";}";
	}
	if (labelAlign != ""){
		switch (labelAlign.toLowerCase()){
