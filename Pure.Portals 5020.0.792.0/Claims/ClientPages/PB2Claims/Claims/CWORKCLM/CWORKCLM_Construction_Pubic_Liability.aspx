			case "centre":
			case "center":
			case "middle": styleString += "#id9bf062396dfd40fa9b48925b4216cb8a label{text-align:center;}"; break;
			case "left": 
			default: styleString += "#id9bf062396dfd40fa9b48925b4216cb8a label{text-align:left;}"; break;
		}
	}
	if (textAlign != ""){
