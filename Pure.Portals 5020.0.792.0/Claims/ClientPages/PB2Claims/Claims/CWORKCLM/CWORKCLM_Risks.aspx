			case "centre":
			case "center":
			case "middle": styleString += "#id7d3968dd411a4454b1167863f368836f input{text-align:center;}"; break;
			case "left": 
			default: styleString += "#id7d3968dd411a4454b1167863f368836f input{text-align:left;}"; break;
		}
	}
	
	if (styleString != ""){
		goog.style.installStyles(styleString);
	}
