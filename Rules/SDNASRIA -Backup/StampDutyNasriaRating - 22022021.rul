Private Sub HandleMidTermTransactions(inceptionDateTpi,lNumberOfDays,lPolicyPeriod,dMinPrem,sRiskCode, dRate, lRiskID)
	Dim sColumns
	Dim vRisksArray
	DIm vExtraParameters
	Dim sDataModelCode
	Dim sGisObject
	DIm sProperty
	Dim lCount
	Dim sRiskDescription
