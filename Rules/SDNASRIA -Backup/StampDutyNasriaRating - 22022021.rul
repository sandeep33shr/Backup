	Dim dPremium
	Dim dEarnerdPremium
	Dim LatestCoverStartDate
	Dim PreviousSumInsured
	Dim vPrevSumInsured	
	Dim bIsSIChanged
	'Get last calculated Premiums
	 sDataModelCode = "SDNASRIA"
	 If sRiskCode = "NPADOM" Then
		sGisObject = "NASRIA_MD"
		sProperty = "MD_"
